{
  "name": "openshift3-base",
  "description": "Openshift3 Common Base Role",
  "json_class": "Chef::Role",
  "default_attributes": {

  },
  "override_attributes": {
    "cookbook-openshift3": {
      "openshift_deployment_type": "origin",
      "ose_major_version": "1.4",
      "ose_version": "1.4.1-1.el7",
      "openshift_common_portal_net": "172.30.0.0/16",
